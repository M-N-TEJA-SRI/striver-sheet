#include <iostream>
using namespace std;
int search(int a[],int n,int key){
    int i;
    for(i=0;i<n;i++){
        if(a[i]==key)
        return i;
    }
    return -1;
}
int main() {
   int n,key;
   cin>>n>>key;
   int a[n],i;
   for(i=0;i<n;i++){
       cin>>a[i];
      }
cout<<search(a,n,key);
    return 0;
}
output
5 2
9 0 5 2 8
3
