#include <iostream>
using namespace std;
int main() {
   int n;
   cin>>n;
   int a[n],i,c=0,maxi=0;
   for(i=0;i<n;i++){
       cin>>a[i];
       if(a[i]==1)
       c++;
       else{
           maxi=max(maxi,c);
           c=0;
       }
      }
      maxi=max(maxi,c);
cout<<maxi;
return 0;
}
output
5
0 1 1 1 0
3
