#include <iostream>
using namespace std;
int main() {
   int n;
   cin>>n;
   int a[n],i,dig[10]={0};
   for(i=0;i<n;i++){
       cin>>a[i];
       dig[a[i]]++;
   }
  
   for(i=0;i<10;i++){
       cout<<i<<" "<<dig[i]<<"\n";
   }

    return 0;
}
output
5
9 0 4 2 3
0 1
1 0
2 1
3 1
4 1
5 0
6 0
7 0
8 0
9 1
